{
    "window.zoomLevel": 0,

    "editor.fontSize": 12,
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "none",

    "files.trimTrailingWhitespace": true,

    "liveshare.featureSet": "insiders",

    "vim.leader": ",",

    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [ "j", "k", ],
            "after": [ "<Esc>" ]
        },
    ],

    "vim.normalModeKeyBindingsNonRecursive": [
        // {
        //     "before": [ "<Leader>", "i", ],
        //     "after": [ "v", "a", "f", "c", "(", "<Esc>", "p", "i", ]
        // },
        {
            "before": [ "<C-x>", ],
            "commands": [ ":q", ],
        },
        {
            "before": [ "<Leader>", "s", ],
            "commands": [ ":w", ],
        },
        {
            "before": [ "-", ],
            "after": [ "d", "d", "p" ],
        },
        {
            "before": [ "z", "j", ],
            "after": [ "z", "b" ]
        },
        {
            "before": [ "z", "k", ],
            "after": [ "z", "t" ]
        },
        {
            "before": [ "<Leader>", "<Space>", ],
            "commands": [ "workbench.action.findInFiles", ],
        },
        {
            "before": [ "<Leader>", "a", "a" ],
            "commands": [ "alternate.alternateFile" ]
        },
        {
            "before": [ "<Leader>", "a", "s" ],
            "commands": [ "alternate.alternateFileInSplit" ]
        },
        {
            "before": [ "]", "<C-d>", ],
            "commands": [ "editor.action.revealDefinition", ],
        },
        {
            "before": [ "[", "<C-d>", ],
            "commands": [ "editor.action.revealDefinition", ],
        },
        {
            "before": [ "<Leader>", "p", "p", ],
            "after": [ "v", "a", "f", ],
            "commands": [ "calva.evaluateSelection", ],
        },
        {
            "before": [ "<Leader>", "P", "P", ],
            "commands": [ "calva.evaluateCurrentTopLevelForm", ],
        },
    ],

    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [ "<Leader>", "p", "p", ],
            "commands": [ "clojureVSCode.evalAndShowResult" ]
        },
        {
            "before": [ ">" ],
            "commands": [ "editor.action.indentLines" ]
        },
        {
            "before": [ "<" ],
            "commands": [ "editor.action.outdentLines" ]
        },
        {
            "before": [ "<Leader>", "p", "p", ],
            "commands": [ "calva.evaluateSelection", ],
        },
        {
            "before": [ "<Leader>", "P", "P", ],
            "commands": [ "calva.evaluateCurrentTopLevelForm", ],
        },
    ],
    "breadcrumbs.enabled": true,
}